{
	"name": "frontend",
	"version": "1.0.0",
	"scripts": {
		"rdeploy": "pnpm sync-env && ssh bbp \" source ~/.nvm/nvm.sh && cd chunavexpress.com/frontend/ && git pull && pnpm i && pnpm build && pnpm start \"",
		"sync-env": "scp .env bbp:chunavexpress.com/frontend/.env ",
		"build": "vite build",
		"dev": "vite",
		"prod": "vite preview",
		"start": "pm2 start ecosystem.config.cjs",
		".copytoremote": "scp -r build/ bbp:chunavexpress.com/frontend/build/ && scp -r ./package.json bbp:chunavexpress.com/frontend/",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch"
	},
	"devDependencies": {
		"@fontsource/fira-mono": "^5.0.8",
		"@lexical/html": "^0.12.5",
		"@lexical/utils": "^0.12.5",
		"@neoconfetti/svelte": "^1.0.0",
		"@sveltejs/adapter-auto": "^2.1.1",
		"@sveltejs/adapter-node": "^1.3.1",
		"@sveltejs/kit": "^1.28.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.1",
		"@types/cookie": "^0.6.0",
		"less": "^4.2.0",
		"lexical": "^0.12.5",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"scss": "^0.2.4",
		"svelte": "^4.2.8",
		"svelte-check": "^3.6.2",
		"svelte-preprocess": "^5.1.1",
		"typescript": "^5.3.3",
		"vite": "^5.0.7",
		"vite-plugin-purgecss": "^0.2.12"
	},
	"type": "module",
	"dependencies": {
		"dotenv": "^16.3.1",
		"md5": "^2.3.0",
		"sveltestrap": "^5.11.2",
		"swiper": "^11.0.5"
	}
}
